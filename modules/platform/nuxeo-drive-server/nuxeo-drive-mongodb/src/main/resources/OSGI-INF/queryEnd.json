          ]
        },
        {
          "$and": [
            {
              "category": "NuxeoDrive"
            },
            {
              "eventId": { "$ne": "rootUnregistered" }
            }
          ]
        }
      ]
    },
    {
      "$and": [
        {
          "_id": { "$gt": ${lowerBound} }
        },
        {
          "_id": { "$lte": ${upperBound} }
        }
      ]
    }
  ]
}
